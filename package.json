{
  "name": "serverless-deployment-manager",
  "version": "0.1.5",
  "description": "Serverless Framework Plugin for managing deployment to predefined AWS accounts",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "compile": "npx babel src --out-dir lib",
    "pre-commit": "npm run compile && npm run lint && npm run prettify",
    "prepublishOnly": "npm run pre-commit && npm test",
    "lint": "eslint . --fix --cache",
    "prettify": "prettier --write --ignore-path .gitignore '**/*.{css,html,js,json,md,yaml,yml}'"
  },
  "author": "laardee",
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "keywords": [
    "serverless"
  ],
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.488.0",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "babel-jest": "^24.8.0",
    "eslint": "^6.0.1",
    "eslint-config-maasglobal": "^2.1.0",
    "eslint-plugin-import": "^2.18.0",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maasglobal/serverless-deployment-manager.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
